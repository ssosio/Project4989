<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boot.sagu.mapper.PhotoMapper">
	<insert id="insertPhoto" parameterType="photo">
		insert into post_photos(post_id,photo_url,is_main) values(#{post_id},#{photo_url},#{is_main})
		<foreach collection="list" item="photo" separator=",">
        (#{photo.postId}, #{photo.photoUrl})
    	</foreach>
	</insert>
</mapper>